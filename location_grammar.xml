<?xml version="1.0" ?>

<grammar xmlns="http://www.w3.org/2001/06/grammar" xml:lang="en-US">
    <rule id="Location" scope="public">
        <one-of>
            <item>
                Santa Cruz
                <tag>
                    out.LocationField = "Santa Cruz";
                </tag>
            </item>
            <item>
                San Francisco
                <tag>
                    out.LocationField = "San Francisco";
                </tag>
            </item>
        </one-of>
    </rule>
</grammar>
